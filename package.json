{
    "name": "texturepacker",
    "version": "0.0.1",
    "description": "Node wrapper around TexturePacker command line tool",
    "main": "index.js",
    "repository": {
        "url":
            "https://github.com/strewhella/node-texturepacker-command-line-wrapper"
    },
    "scripts": {
