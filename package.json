    "license": "MIT",
    "prettier": {
        "tabWidth": 4,
        "singleQuote": true,
        "semi": true
    },
    "dependencies": {
        "babel-cli": "^6.26.0",
        "babel-preset-env": "^1.6.1",
        "jest": "^22.4.3"
    }
}
